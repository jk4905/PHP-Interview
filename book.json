{
  "title": "PHP-Interview",
  "author": "Kagami丶",
  "description": "Kagami丶 的 PHP 复习笔记",
  "language": "zh-hans",
  "output.name": "docs",
  "gitbook": "3.2.3",
  "root":".",
  "bookname": "Kagami丶PHP-Interview",
  "outputfile": "SUMMARY.md",
  "catalog": "all",
  "ignores": [
    "node_modules",
    "docs"
  ],
  "unchanged": [],
  "styles": {
    "website": "./styles/website.css"
  },
  "plugins": [
    "theme-comscore",
    "prism",
    "-highlight",
    "copy-code-button",
    "search-pro",
    "-search",
    "-lunr",
    "expandable-chapters",
    "splitter",
    "-sharing",
    "github-buttons",
    "tbfed-pagefooter",
    "anchor-navigation-ex",
    "disqus",
    "github",
    "hide-element",
    "insert-logo",
    "pageview-count",
    "popup"
  ],
  "pluginsConfig": {
    "hide-element": {
        "elements": [
          ".gitbook-link"
        ]
    },
    "insert-logo": {
        "url": "https://raw.githubusercontent.com/jk4905/PHP-Interview/master/docs/images/logo.png",
        "style": "background: none; max-height: 30px; min-height: 30px"
    },
    "disqus": {
        "shortName": "jk4905"
    },
    "github": {
        "url": "https://github.com/jk4905"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "jk4905",
          "repo": "PHP-Interview", 
          "type": "star",
          "count": true,
          "size": "small"
        }, 
        {
          "user": "jk4905",
          "width": "160", 
          "type": "follow", 
          "count": true,
          "size": "small"
        }
      ]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ],
      "lang": {
        "shell": "bash"
      }
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy Kagami丶 2019",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "anchor-navigation-ex": {
      "showLevel": true
    }
  }
}
